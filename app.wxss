/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{
    background:#fafafa;
}
@font-face {
  font-family: 'ksico';
  src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

[class^="ico-"], [class*=" ico-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'ksico' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.ico-images:before {
  content: "\e90e";
}
.ico-camera:before {
  content: "\e90f";
}
.ico-file-picture:before {
  content: "\e927";
}
.ico-history:before {
  content: "\e94d";
}
.ico-clock:before {
  content: "\e94e";
}
.ico-drawer:before {
  content: "\e95c";
}
.ico-box-remove:before {
  content: "\e95f";
}
.ico-spinner7:before {
  content: "\e980";
}
.ico-trophy:before {
  content: "\e99e";
}


.lines{
    display:flex;
    align-items:center;
    padding:32rpx 0;
    margin:0 30rpx;
    justify-content:space-between;
    font-size:28rpx;
    color:#999;
    position:relative;
    background:#fff;
}
.lines:after{
    content:'';
    position:absolute;
    left:0;
    right:0;
    height:1px;
    bottom:0;
    transform-origin:0 100%;
    background:#eee;
    transform:scale(.5);
    width:200%;

}

.lines:last-child::after{
    border:none;
}
.lines text{
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.lines .value{
    font-size:30rpx;
    color:#444;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    text-align:right;
}
.btnbar {
    text-align:center;
    padding:20px 0 60px;
}
.btnbar .info{
    margin:0 30rpx 50rpx;
    font-size:24rpx;
    text-align:left;
    color:#27CFB1;
}
.btnbar .btn{
    display:inline-block;
    width:40%;
    padding:8px 0;
    font-size:36rpx;
    background:#27CFB1;color:#fff;
    margin:0 5px;
    border-radius:4rpx;
    vertical-align:top;
    line-height:normal;
}

.btnbar .btns-hover{
    background:#ccc;
    color:#888;
}

